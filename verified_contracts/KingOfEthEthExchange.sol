&#13;
        // Burn ore&#13;
        KingOfEthResource(oreContract).interfaceBurnTokens(&#13;
              _player&#13;
            , _length&#13;
        );&#13;
    }&#13;
&#13;
    /// @dev The exchange can freeze tokens&#13;
